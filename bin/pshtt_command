#!/Users/ericschles/Documents/projects/devops_work/pshtt_command/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'pshtt-command','console_scripts','pshtt_command'
__requires__ = 'pshtt-command'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('pshtt-command', 'console_scripts', 'pshtt_command')()
    )
